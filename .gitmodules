#The external libraries that the LWGLGF depends on are found at the following URLS.
#With that in mind, adding them as submodules of this project might make life easier for developing the project, especially for getting bleeding-edge changes.
[submodule "extern/glm"]
	path = extern/glm
	url = https://github.com/g-truc/glm.git
[submodule "extern/glew"]
	path = extern/glew
	url = https://github.com/nigels-com/glew.git
[submodule "extern/glfw"]
	path = extern/glfw
	url = https://github.com/glfw/glfw.git
